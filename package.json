    "frame": true,
    "width": 1024,
    "height": 520,
    "fullscreen": false,
    "icon": "app/common/images/appicon.png",
    "position": "center"
  },
  "platformOverrides": {
    "win": {
      "window": {
        "toolbar": false
      }
    },
    "osx": {
      "window": {
        "toolbar": false
      }
    },
    "linux": {
      "window": {
        "toolbar": false
      }
    }
