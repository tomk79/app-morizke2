    "build": "node ./build.js"
  },
  "author": "Tomoya Koyanagi",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^0.22.0",
    "ejs": "^2.5.2",
    "express": "^4.13.0",
    "file-stream-rotator": "0.0.7",
    "fs-extra": "^0.24.0",
    "glob": "^5.0.10",
    "iterate79": "^1.0.0",
    "jquery": "^2.1.4",
    "langbank": "0.0.2",
    "marked": "0.3.*",
    "mkdirp": "0.*",
    "morgan": "^1.7.0",
    "node-php-bin": "^0.2.3",
    "node-search-in-directory": "0.0.1",
    "phpjs": "^1.3.2",
    "px2agent": "file:submodules/node-px2agent",
    "px2dt-localdata-access": "file:submodules/node-px2dt-localdata-access",
